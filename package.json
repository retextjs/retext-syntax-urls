{
  "name": "retext-syntax-urls",
  "version": "1.0.0",
  "description": "Classify url-like values (example.com, example.md, etc) as syntax",
  "license": "MIT",
  "keywords": [
    "syntax",
    "source",
    "url",
    "filename",
    "path",
    "retext"
  ],
  "repository": "wooorm/retext-syntax-urls",
  "bugs": "https://github.com/wooorm/retext-syntax-urls/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "nlcst-to-string": "^2.0.0",
    "unist-util-modify-children": "^1.1.1",
    "unist-util-position": "^3.0.0"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.0",
    "is-hidden": "^1.1.1",
    "not": "^0.1.0",
    "nyc": "^11.0.0",
    "remark-cli": "^4.0.0",
    "remark-preset-wooorm": "^3.0.0",
    "retext": "^5.0.0",
    "tape": "^4.0.0",
    "unist-util-remove-position": "^1.0.0",
    "unist-util-visit": "^1.1.3",
    "xo": "^0.18.0"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build-bundle": "browserify index.js --bare -s retextSyntaxUrls > retext-syntax-urls.js",
    "build-mangle": "esmangle retext-syntax-urls.js > retext-syntax-urls.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "ignores": [
      "retext-syntax-urls.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
